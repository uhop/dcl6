{
  "name": "dcl6",
  "version": "6.0.0",
  "description": "Elegant minimalistic implementation of OOP with mixins + AOP.",
  "author": "Eugene Lazutkin <eugene.lazutkin@gmail.com> (http://lazutkin.com/)",
  "license": "BSD-3-Clause",
  "homepage": "http://www.dcljs.org",
  "bugs": "http://github.com/uhop/dcl6/issues",
  "main": "dcl.js",
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "heya-globalize": "^1.2.1",
    "heya-unit": "^0.3.0"
  },
  "scripts": {
    "test": "node tests/tests.js",
    "cover": "istanbul cover tests/tests.js && cat ./coverage/lcov.info | coveralls",
    "dist-g": "node node_modules/heya-globalize/index.js",
    "dist-es6": "node node_modules/heya-globalize/index.js --es6 --target=es6",
    "dist-es5": "babel es6/ -d lib/",
    "clean": "rm -fr dist/ es6/ lib/",
    "dist": "npm run clean && npm run dist-g && npm run dist-es6 && npm run dist-es5",
    "prepublish": "npm run dist"
  },
  "github": "http://github.com/uhop/dcl6",
  "repository": {
    "type": "git",
    "url": "git://github.com/uhop/dcl6.git"
  },
  "keywords": [
    "object-oriented",
    "programming",
    "aspect-oriented",
    "OOP",
    "AOP",
    "OO"
  ],
  "browserGlobals": {
    "!root": "dcl",
    "./dcl": "dcl",
    "./debug": "!dcl"
  }
}
